{
    "name": "landingi/core",
    "description": "Landingi Standard Library",
    "type": "library",
    "license": "MIT",
    "version": "3.0.0",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12",
        "symplify/easy-coding-standard": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Landingi\\Core\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Landingi\\Core\\" : "tests"
        }
    },
    "minimum-stability": "stable"
}
